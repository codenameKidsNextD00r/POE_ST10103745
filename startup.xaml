<Window x:Class="POE_ST10103745.startup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POE_ST10103745"
        mc:Ignorable="d"
        Title="startup" Height="450" Width="800">
    <Grid>
        <GroupBox Header="Welcome" Margin="102,56,102,55"/>
        <Label Content="Sign Up (new user) or Login if you already have an account:" HorizontalAlignment="Center" Margin="0,103,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btn_Register" Content="Sign Up" HorizontalAlignment="Left" Height="26" Width="53" Margin="322,270,0,0" VerticalAlignment="Top" Click="btn_Register_Click"/>
        <Button x:Name="btn_Login" Content="Login" HorizontalAlignment="Left" Height="26" Width="53" Margin="426,270,0,0" VerticalAlignment="Top" Click="btn_Login_Click"/>


    </Grid>
</Window>
